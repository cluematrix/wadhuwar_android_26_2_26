<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:visibility="gone"
    android:id="@+id/lifestyle_appearance_LL"
    android:paddingBottom="10dp"
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:layout_marginLeft="20dp"
        android:layout_marginTop="25dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Lifestyle &amp; Appearance"
        android:textColor="@color/description_text"
        android:textSize="19sp"
        android:textStyle="bold" />

    <androidx.cardview.widget.CardView
        android:layout_marginBottom="2dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:elevation="4dp">

        <LinearLayout
            android:padding="13dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="6dp"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:background="@drawable/blue_person"
                    />



                <TextView
                    android:id="@+id/age_height_color"
                    android:layout_marginLeft="20dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="16sp"
                    android:textColor="@color/dark_gray" />

            </LinearLayout>


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="25dp"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:background="@drawable/ic_cake"
                    />


                <TextView
                    android:id="@+id/birthday_date"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="20dp"
                    android:text="Born On **/**/****"
                    android:textColor="@color/dark_gray"

                    android:textSize="16sp" />

                <ImageView
                    android:id="@+id/birthday_lock"
                    android:layout_marginLeft="8dp"
                    android:layout_width="19dp"
                    android:layout_height="19dp"
                    android:background="@drawable/lock_red"
                    />

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="25dp"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:background="@drawable/baseline_access_time_24"
                    />

                <TextView
                    android:id="@+id/birthday_time"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="20dp"
                    android:text="Born time **:**"
                    android:textColor="@color/dark_gray"

                    android:textSize="16sp" />

                <ImageView
                    android:id="@+id/birthday_time_lock"
                    android:layout_marginLeft="8dp"
                    android:layout_width="19dp"
                    android:layout_height="19dp"
                    android:background="@drawable/lock_red"
                    />

            </LinearLayout>


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="25dp"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:background="@drawable/ic_ring"
                    />


                <TextView
                    android:id="@+id/marital_status"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="20dp"
                    android:text="Marital status ******"
                    android:textColor="@color/dark_gray"
                    android:textSize="16sp" />

                <ImageView
                    android:id="@+id/marital_status_lock"
                    android:layout_marginLeft="8dp"
                    android:layout_width="19dp"
                    android:layout_height="19dp"
                    android:background="@drawable/lock_red"
                    />

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="25dp"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:background="@drawable/red_location_24"
                    />



                <TextView
                    android:id="@+id/address_lifestyle"
                    android:layout_marginLeft="20dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textSize="16sp"

                    android:textColor="@color/dark_gray" />

            </LinearLayout>


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="25dp"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:background="@drawable/ic_portfolio_filled_hand_drawn_tool"
                    />



                <TextView
                    android:id="@+id/job_lifestyle"
                    android:layout_marginLeft="20dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="16sp"

                    android:textColor="@color/dark_gray" />

            </LinearLayout>


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="25dp"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:background="@drawable/dining_green"
                    />



                <TextView
                    android:id="@+id/diet"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="20dp"
                    android:text="Diet ******"
                    android:textColor="@color/dark_gray"
                    android:textSize="16sp" />

                <ImageView
                    android:id="@+id/diet_lock"
                    android:layout_marginLeft="8dp"
                    android:layout_width="19dp"
                    android:layout_height="19dp"
                    android:background="@drawable/lock_red"
                    />


            </LinearLayout>


            <LinearLayout
                android:visibility="gone"
                android:id="@+id/contact_go_premium_now_btn_LL"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical">

                <View
                    android:layout_marginRight="20dp"
                    android:layout_marginLeft="20dp"
                    android:layout_height="0.5dp"
                    android:layout_width="match_parent"
                    android:layout_marginTop="15dp"
                    android:layout_marginBottom="20dp"
                    android:background="@color/Gray"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="To unlock Birth Date, Marital Status &amp; Diet"
                    android:textSize="15sp"
                    android:layout_gravity="center_horizontal"
                    android:textColor="@color/description_text" />



                <LinearLayout
                    android:id="@+id/contact_go_premium_now_btn"
                    android:layout_gravity="center"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal"
                    android:gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/curved_rectangle_blue_border"
                    android:paddingLeft="25dp"
                    android:paddingTop="9dp"
                    android:paddingRight="25dp"
                    android:paddingBottom="9dp">



                    <TextView

                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:text="Go Premium Now"
                        android:textColor="@color/pharma"
                        android:textSize="16sp"
                        android:textStyle="bold" />

                </LinearLayout>

            </LinearLayout>





        </LinearLayout>
    </androidx.cardview.widget.CardView>



</LinearLayout>